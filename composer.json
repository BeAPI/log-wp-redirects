{
    "name": "beapi/log-wp-redirects",
    "description": "Log all WordPress redirections made via wp_redirect() function",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Be API",
            "email": "technical@beapi.fr",
            "homepage": "https://beapi.fr"
        }
    ],
    "require": {
        "php": ">=7.0",
        "composer/installers": "^1.0 || ^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "wp-coding-standards/wpcs": "^2.3"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "keywords": [
        "wordpress",
        "plugin",
        "redirect",
        "logging",
        "wp_redirect",
        "monitor",
        "debug"
    ],
    "support": {
        "issues": "https://github.com/BeAPI/log-wp-redirects/issues",
        "source": "https://github.com/BeAPI/log-wp-redirects"
    },
    "minimum-stability": "stable"
} 